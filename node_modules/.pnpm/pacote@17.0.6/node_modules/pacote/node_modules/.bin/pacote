#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rezajafar/peakofeloquence.org/node_modules/.pnpm/pacote@17.0.6/node_modules/pacote/lib/node_modules:/Users/rezajafar/peakofeloquence.org/node_modules/.pnpm/pacote@17.0.6/node_modules/pacote/node_modules:/Users/rezajafar/peakofeloquence.org/node_modules/.pnpm/pacote@17.0.6/node_modules:/Users/rezajafar/peakofeloquence.org/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rezajafar/peakofeloquence.org/node_modules/.pnpm/pacote@17.0.6/node_modules/pacote/lib/node_modules:/Users/rezajafar/peakofeloquence.org/node_modules/.pnpm/pacote@17.0.6/node_modules/pacote/node_modules:/Users/rezajafar/peakofeloquence.org/node_modules/.pnpm/pacote@17.0.6/node_modules:/Users/rezajafar/peakofeloquence.org/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/bin.js" "$@"
else
  exec node  "$basedir/../../lib/bin.js" "$@"
fi
