#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rezajafar/peakofeloquence.org/node_modules/.pnpm/listhen@1.7.2/node_modules/listhen/bin/node_modules:/Users/rezajafar/peakofeloquence.org/node_modules/.pnpm/listhen@1.7.2/node_modules/listhen/node_modules:/Users/rezajafar/peakofeloquence.org/node_modules/.pnpm/listhen@1.7.2/node_modules:/Users/rezajafar/peakofeloquence.org/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rezajafar/peakofeloquence.org/node_modules/.pnpm/listhen@1.7.2/node_modules/listhen/bin/node_modules:/Users/rezajafar/peakofeloquence.org/node_modules/.pnpm/listhen@1.7.2/node_modules/listhen/node_modules:/Users/rezajafar/peakofeloquence.org/node_modules/.pnpm/listhen@1.7.2/node_modules:/Users/rezajafar/peakofeloquence.org/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../listhen@1.7.2/node_modules/listhen/bin/listhen.mjs" "$@"
else
  exec node  "$basedir/../../../../../listhen@1.7.2/node_modules/listhen/bin/listhen.mjs" "$@"
fi
